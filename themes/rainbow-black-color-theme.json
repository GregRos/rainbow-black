{
  "name": "Rainbow Black",
  "type": "dark",
  "colors": {
    "editor.background": "#263238",
    "editor.foreground": "#eeffff",
    "activityBarBadge.background": "#007acc",
    "sideBarTitle.foreground": "#bbbbbb"
  },
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "builtinConstant": {
      "foreground": "#4276ed"
    },
    "*.typeHint": {
      "italic": true
    },
    "class": {
      "bold": true,
      "foreground": "#54e050"
    },
    "struct": {
      "bold": true,
      "foreground": "#54e050"
    },
    "class.declaration": {
      "underline": true
    },
    "class.builtin": {
      "bold": true,
      "foreground": "#ffea00"
    },
    "class.decorator.builtin": {
      "bold": false,
      "foreground": "#e900d1"
    },
    "enum": {
      "bold": true,
      "foreground": "#83dec2"
    },
    "function": {
      "foreground": "#d63c3c"
    },
    "function.declaration": {
      "underline": true
    },

    "function.builtin": {
      "foreground": "#f3baaa",
      "bold": true
    },
    "function.decorator": {
      "bold": false,
      "foreground": "#e900d1"
    },
    "function.defaultLibrary": {
      "foreground": "#f66595"
    },
    "interface": {
      "fontStyle": "bold",
      "foreground": "#90abff"
    },
    "magicFunction.builtin": {
      "foreground": "#c562ff"
    },
    "magicFunction.declaration": {
      "foreground": "#c562ff"
    },
    "method": {
      "foreground": "#d63c3c"
    },
    "method.declaration": {
      "underline": true
    },
    "module": {
      "bold": true,
      "foreground": "#66ff00"
    },
    "namespace": {
      "bold": true,
      "foreground": "#52f0ac"
    },
    "parameter": {
      "bold": false,
      "foreground": "#ffffff",
      "italic": true
    },
    "parameter.declaration": {
      "underline": true
    },
    "property": {
      "foreground": "#7cf86f"
    },
    "property.declaration:python": {
      "underline": true
    },
    "selfParameter": {
      "foreground": "#9ce5ff"
    },
    "type": {
      "bold": true,
      "foreground": "#00d9ff"
    },
    "typeParameter": {
      "bold": true,
      "foreground": "#ff8000",
      "italic": true
    },
    "variable": {
      "foreground": "#ffffff",
      "bold": true
    },
    "variable.readonly": {
      "foreground": "#ffffff",
      "bold": false
    },
    "variable.defaultLibrary": {
      "bold": true,
      "foreground": "#80fb92"
    },
    "variable.typeHint": {
      "foreground": "#ff8000",
      "bold": true
    }
  },
